{
  "name": "@skmtc/cli-ink",
  "version": "0.0.1",
  "exports": "./mod.ts",
  "imports": {
    "@/": "./",
    "@skmtc/core": "jsr:@skmtc/core@^0.0.819",
    "react": "npm:react@^19.0.0",
    "react/jsx-runtime": "npm:react@^19.0.0/jsx-runtime",
    "ink": "npm:ink@^6.0.0",
    "@inkjs/ui": "npm:@inkjs/ui@^2.0.0",
    "ink-testing-library": "npm:ink-testing-library@^4.0.0",
    "ink-divider": "npm:ink-divider@^4.1.1",
    "ink-link": "npm:ink-link@^4.1.0",
    "@mishieck/ink-titled-box": "npm:@mishieck/ink-titled-box@^0.3.0"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "jsxImportSourceTypes": "npm:@types/react@^19"
  },
  "tasks": {
    "publish:ga": "deno task build && deno task publish:deno:ga && deno task publish:npm",
    "publish": "deno task build && deno task publish:deno && deno task publish:npm",
    "build": "deno run -A .scripts/build-node.ts",
    "publish:deno": "deno publish --allow-slow-types --allow-dirty --token=$JSR_AUTH_TOKEN",
    "publish:deno:ga": "deno publish --allow-slow-types",
    "publish:npm": "cd ../../packages/cli && npm publish",
    "test": "deno test --allow-all --no-check tests/",
    "test:components": "deno test --allow-all --no-check tests/components/",
    "test:watch": "deno test --allow-all --watch tests/",
    "test:coverage": "deno test --allow-all --coverage tests/"
  }
}
