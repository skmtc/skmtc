{
  "name": "@skmtc/core",
  "version": "0.0.474", 
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.9",
    "@std/testing": "jsr:@std/testing@^1.0.4",
    "@types/lodash-es": "npm:@types/lodash-es@4.17.12",
    "lodash-es": "npm:lodash-es@4.17.21"
  },
  "exports": {
    ".": "./mod.ts",
    "./Brand": "./types/Brand.ts",
    "./Extensions": "./types/Extensions.ts",
    "./GeneratorKeys": "./types/GeneratorKeys.ts",
    "./Manifest": "./types/Manifest.ts",
    "./Method": "./types/Method.ts",
    "./RefName": "./types/RefName.ts",
    "./Results": "./types/Results.ts",
    "./toResolvedArtifactPath": "./helpers/toResolvedArtifactPath.ts",
    "./strings": "./helpers/strings.ts",
    "./Settings": "./types/Settings.ts"
  },
  "tasks": {
    "publish": "deno task build && deno task publish:deno && deno task publish:npm",
    "build": "deno run -A .scripts/build-node.ts",
    "publish:deno": "deno publish --allow-slow-types --allow-dirty",
    "publish:npm": "cd .npm && npm publish"
  },
  "test": {
    "exclude": [".scripts/", ".npm/"]
  }
}
