{
  "name": "@skmtc/core",
  "version": "0.0.662",
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.10",
    "@std/testing": "jsr:@std/testing@^1.0.8",
    "@std/yaml": "jsr:@std/yaml@^1.0.5",
    "@std/log": "jsr:@std/log@0.224.6",
    "@types/lodash-es": "npm:@types/lodash-es@4.17.12",
    "lodash-es": "npm:lodash-es@4.17.21",
    "@google/generative-ai": "npm:@google/generative-ai@^0.21.0",
    "@sentry/deno": "npm:@sentry/deno@8.47.0",
    "openapi-types": "npm:openapi-types@12.1.3",
    "valibot": "npm:valibot@1.0.0",
    "prettier": "npm:prettier@^3.5.3",
    "make-synchronized": "npm:make-synchronized@^0.7.2"
  },
  "exports": {
    ".": "./mod.ts",
    "./Brand": "./types/Brand.ts",
    "./EnrichmentRequest": "./types/EnrichmentRequest.ts",
    "./GeneratorKeys": "./types/GeneratorKeys.ts",
    "./Manifest": "./types/Manifest.ts",
    "./Method": "./types/Method.ts",
    "./Preview": "./types/Preview.ts",
    "./RefName": "./types/RefName.ts",
    "./Results": "./types/Results.ts",
    "./Settings": "./types/Settings.ts",
    "./strings": "./helpers/strings.ts",
    "./toResolvedArtifactPath": "./helpers/toResolvedArtifactPath.ts"
  },
  "tasks": {
    "publish": "deno task build && deno task publish:deno && deno task publish:npm",
    "build": "deno run -A .scripts/build-node.ts",
    "publish:deno": "deno publish --allow-slow-types --allow-dirty",
    "publish:npm": "cd ../../packages/core && npm publish"
  },
  "test": {
    "exclude": [".scripts/"]
  }
}
 