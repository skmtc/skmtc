    "openapi-types": "npm:openapi-types@^12.1.3",
    "swagger2openapi": "npm:swagger2openapi@^7.0.8",
    "tiny-invariant":"npm:tiny-invariant@1.3.3",
    "ts-pattern": "npm:ts-pattern@^5.8.0",
    "unique-names-generator": "npm:unique-names-generator@^4.7.1",
    "valibot": "npm:valibot@1.1.0",
    "octokit": "npm:octokit@^5.0.3",
    "zod": "npm:zod@^4.0.14"
  },
  "unstable": ["kv"],
  "workspace": [
    "./cli",
    "./core",
    "./mcp",
    "./server"
  ],
  "tasks": {
    "release": "deno check ./**/*.ts && deno task increment-patch && deno task publish",
    "publish": "deno task --cwd=core publish && deno task --cwd=cli publish && deno task --cwd=server publish",
    "increment-patch": "deno run --allow-read --allow-write .scripts/increment-patch.ts"
  }
}