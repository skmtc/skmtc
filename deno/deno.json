{
  "imports":{
    "@apiture/openapi-down-convert": "npm:@apiture/openapi-down-convert@0.14.0",
    "@babel/helper-validator-identifier": "npm:@babel/helper-validator-identifier@7.27.1",
    "@cliffy/command": "jsr:@cliffy/command@1.0.0-rc.8",
    "@cliffy/internal": "jsr:@cliffy/internal@1.0.0-rc.8",
    "@cliffy/keycode": "jsr:@cliffy/keycode@1.0.0-rc.8",
